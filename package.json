{
  "name": "request-dot-js",
  "version": "3.0.4",
  "author": "Kyle Bebak <kylebebak@gmail.com>",
  "description": "A ~1kB wrapper around `fetch` with convenient error handling, automatic JSON transforms, and support for exponential backoff.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "fetch",
    "request",
    "JSON",
    "backoff",
    "retry"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fortana-co/request.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.0.8",
    "ava": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "husky": "^1.3.1",
    "node-fetch": "^2.3.0",
    "prettier": "1.16.4",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "npm run test",
    "test": "tsc && prettier --check *.ts && ava -v",
    "prettier-check": "prettier --check *.ts",
    "prettier": "prettier --write *.ts"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-push": "npm run prettier-check"
    }
  }
}
