{
  "name": "request-dot-js",
  "version": "2.3.2",
  "author": "Kyle Bebak <kylebebak@gmail.com>",
  "description": "A ~1kB wrapper around `fetch` with convenient error handling, automatic JSON transforms, and support for exponential backoff.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "fetch",
    "request",
    "JSON",
    "backoff",
    "retry"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fortana-co/request.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "husky": "^1.3.1",
    "node-fetch": "^2.3.0",
    "prettier": "1.16.4",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "test": "tsc && eslint index.js stringify.js && prettier --check index.js stringify.js && ava -v",
    "prettier-check": "prettier --check index.js stringify.js",
    "prettier": "prettier --write index.js stringify.js",
    "postinstall": "rm -f .git/hooks/applypatch-msg & rm -f .git/hooks/commit-msg & rm -f .git/hooks/prepare-commit-msg"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-push": "npm run prettier-check"
    }
  }
}
