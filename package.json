{
  "name": "request-dot-js",
  "version": "2.1.0",
  "author": "Kyle Bebak <kylebebak@gmail.com>",
  "description": "A ~1kB wrapper around fetch, with a convenient API for interacting with JSON APIs and support for exponential backoff",
  "main": "./index.js",
  "keywords": [
    "fetch",
    "request",
    "JSON",
    "backoff",
    "retry"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fortana-co/request.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "husky": "^1.3.1",
    "node-fetch": "^2.3.0",
    "prettier": "1.16.4"
  },
  "scripts": {
    "test": "eslint index.js stringify.js && prettier --check index.js stringify.js && ava -v",
    "prettier-check": "prettier --check index.js stringify.js",
    "prettier": "prettier --write index.js stringify.js"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-push": "npm run prettier-check"
    }
  }
}
